    <!-- https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/Mood%20Congruent%20Memory/Mood-Congruent%20Memory.owl#Anxiety -->

    <owl:NamedIndividual rdf:about="https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/Mood%20Congruent%20Memory/Mood-Congruent%20Memory.owl#Anxiety">
        <rdf:type rdf:resource="https://softeng.polito.it/rsctx/#EmotionalState"/>
        <reaction:isConsequenceOf rdf:resource="https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/Mood%20Congruent%20Memory/Mood-Congruent%20Memory.owl#HospitalStaying"/>
