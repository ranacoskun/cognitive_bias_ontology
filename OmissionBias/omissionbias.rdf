        <rdfs:comment xml:lang="en">Inverse of raise.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">http://www.ontologydesignpatterns.org/cp/owl/reaction.owl.v0.1#</rdfs:isDefinedBy>
