        <rdfs:isDefinedBy xml:lang="en">https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/OmissionBias/omissionbias.owl#Comission</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">Comission</rdfs:label>
    </owl:Class>
    


